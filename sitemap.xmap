<?xml version="1.0" encoding="UTF-8"?>
<map:sitemap xmlns:map="http://apache.org/cocoon/sitemap/1.0">
<map:components>

	<map:generators default="file">
		<map:generator name="testsession" src="Server.Test.TestSession"/>
<!--
		<map:generator name="sample" src="Server.Sample.Sample"/>
		<map:generator name="monitor-server" src="Server.Sample.MonitorServer"/>
-->

		<map:generator name="opensignin" src="Server.Signin.OpenSignin"/>
		<map:generator name="signin" src="Server.Signin.Signin"/>
		<map:generator name="signout" src="Server.Signin.Signout"/>
		<map:generator name="account" src="Server.Signin.Account"/>
		<map:generator name="password" src="Server.Signin.Password"/>

		<map:generator name="vicar" src="Server.Core.Vicar"/>
		<map:generator name="filedownload" src="Server.Core.FileDownload"/>

		<map:generator name="ajax-upload" src="Server.Upload.AjaxServer"/>

		<map:generator name="stream-server" src="Server.Convert.StreamServer"/>
	</map:generators>


	<map:transformers default="xslt"/>
	<map:readers default="resource"/>

	<map:serializers default="html">
		<!--script elements need inner text of &#160; to work with serializer - weird -->
		<map:serializer name="html5"
			mime-type="text/html"
			src="org.apache.cocoon.serialization.XMLSerializer">
			<doctype-public></doctype-public>
			<doctype-system>about:legacy-compat</doctype-system>
			<encoding>UTF-8</encoding>
			<indent>yes</indent>
		</map:serializer>
	</map:serializers>

	<map:actions />

	<map:selectors default="browser"/>
	<map:matchers default="wildcard"/>
</map:components>


<map:pipelines>
<map:pipeline>

<!--TOP-->
<map:match pattern="" >
	<map:redirect-to uri="Vicar.html"/>
</map:match>
<map:match pattern="index.html" >
	<map:redirect-to uri="Vicar.html"/>
</map:match>

<!--TEST-->
<map:match pattern="Test/**">
	<map:match pattern="Test/TestSession.html" >
		<map:generate type="testsession"/>
		<map:transform src="Server/Test/TestSession.xsl"/>
		<map:serialize type="html5"/>
	</map:match>
</map:match>

<!--JAVADOC-->
<map:match pattern="javadocs/**">
	<map:match pattern="javadocs/**.html" >
		<map:read mime-type="text/html" src="javadocs/{1}.html" />
	</map:match>
	<map:match pattern="javadocs/**.css" >
		<map:read mime-type="text/css" src="javadocs/{1}.css" />
	</map:match>
	<map:match pattern="javadocs/**.js" >
		<map:read mime-type="text/javascript" src="javadocs/{1}.js" />
	</map:match>
	<map:match pattern="javadocs/**.gif" >
		<map:read mime-type="image/gif" src="javadocs/{1}.gif" />
	</map:match>
</map:match>

<!--SAMPLE-->
<!--
<map:match pattern="Sample/**">
	<map:match pattern="Sample/Sample.html" >
		<map:generate type="sample"/>
		<map:transform src="Server/Sample/Sample.xsl"/>
		<map:serialize type="html5"/>
	</map:match>
	<map:match pattern="Sample/Sample.xml" >
		<map:generate type="sample"/>
		<map:serialize type="xml"/>
	</map:match>
	<map:match pattern="Sample/Sample.css">
		<map:read mime-type="text/css" src="Server/Signin/Sample.css"/>
	</map:match>
	<map:match pattern="Upload/MonitorServer.xml" >
		<map:generate type="monitor-server"/>
		<map:serialize type="xml"/>
	</map:match>
</map:match>
-->

<!--MODERNIZR-->
<map:match pattern="Modernizr/**">
	<map:match pattern="Modernizr/modernizr.js" >
		<map:read mime-type="text/javascript" src="Server/Modernizr/modernizr.js" />
	</map:match>
</map:match>

<!--DOWNLOAD-->
<map:match pattern="Download/**">
	<map:match pattern="Download/*/*.xml" >
		<map:generate type="filedownload" />
		<map:serialize type="xml" mime-type="application/octet-stream" />
	</map:match>
	<map:match pattern="Download/*/*.zip" >
		<map:generate type="filedownload" />
		<map:serialize type="zip" mime-type="application/octet-stream" />
	</map:match>
	<map:match pattern="Download/*/*.tar" >
		<map:generate type="filedownload" />
		<map:serialize mime-type="application/octet-stream" />
	</map:match>
	<map:match pattern="Download/*/*.tar.gz" >
		<map:generate type="filedownload" />
		<map:serialize mime-type="application/octet-stream" />
	</map:match>
	<map:redirect-to uri="../Error.html"/>
</map:match>

<map:match pattern="Utils/**">
	<map:match pattern="Utils/AjaxClient.js">
		<map:read mime-type="text/javascript" src="Server/Utils/AjaxClient.js"/>
	</map:match>
</map:match>

<!--UPLOAD-->
<map:match pattern="Upload/**">

	<!--MULTIPLE FILE UPLOAD VIA AJAX-->
	<map:match pattern="Upload/AjaxUpload.js">
		<map:read mime-type="text/javascript" src="Server/Upload/AjaxUpload.js"/>
	</map:match>
	<map:match pattern="Upload/PopupFrame.js">
		<map:read mime-type="text/javascript" src="Server/Upload/PopupFrame.js"/>
	</map:match>
	<map:match pattern="Upload/PopupFrame.css">
		<map:read mime-type="text/css" src="Server/Upload/PopupFrame.css"/>
	</map:match>
	<map:match pattern="Upload/AjaxUpload.css">
		<map:read mime-type="text/css" src="Server/Upload/AjaxUpload.css"/>
	</map:match>
	<map:match pattern="Upload/AjaxServer.xml" >
		<map:generate type="ajax-upload"/>
		<map:serialize type="xml"/>
	</map:match>

	<map:redirect-to uri="../Error.html"/>
</map:match>

<map:match pattern="Convert/**">
	<!--MULTIPLE FILE UPLOAD VIA AJAX-->
	<map:match pattern="Convert/StreamClient.html">
		<map:generate src="Server/Convert/StreamClient.xml" />
		<map:transform src="Server/Convert/StreamClient.xsl">
			<map:parameter name="use-request-parameters" value="true" />
		</map:transform>
		<map:serialize type="html"/>
	</map:match>
	<map:match pattern="Convert/StreamClient.js">
		<map:read mime-type="text/javascript" src="Server/Convert/StreamClient.js"/>
	</map:match>

	<map:match pattern="Convert/StreamServer.html" >
		<map:generate type="stream-server"/>
		<map:transform src="Server/Convert/StreamServer.xsl" />
		<map:serialize type="html"/>
	</map:match>
	<map:match pattern="Convert/StreamServer.xml" >
		<map:generate type="stream-server"/>
		<map:serialize type="xml"/>
	</map:match>

	<map:redirect-to uri="../Error.html"/>
</map:match>

<!--VALID - THIS ALLOWS DIRECT LINKING TO THE VALIDATION HTML OUTPUT-->
<map:match pattern="valid/**">
	<map:match pattern="valid/*/*.html" >
		<map:generate type="filedownload"/>
		<map:serialize type="html" />
	</map:match>
</map:match>

<map:match pattern="Error.html">
	<map:match pattern="Error.html">
		<map:read mime-type="text/html" src="Server/Error.html"/>
	</map:match>
</map:match>

<!--TOP LEVEL URL PATHS FOR SIGNIN AND CORE FUNCTIONALITY -->
<map:match pattern="**">
	<map:match pattern="AboutVicar.html" >
		<map:read mime-type="text/html" src="AboutVicar.html" />
	</map:match>
	<map:match pattern="VicarSoftware.html" >
		<map:read mime-type="text/html" src="VicarSoftware.html" />
	</map:match>

	<map:match pattern="Signin.html" >
		<map:generate type="signin"/>
		<map:transform src="Server/Signin/Signin.xsl"/>
		<map:serialize type="html5"/>
	</map:match>
	<map:match pattern="Account.html" >
		<map:generate type="account"/>
		<map:transform src="Server/Signin/Account.xsl"/>
		<map:serialize type="html5"/>
	</map:match>
	<map:match pattern="Password.html" >
		<map:generate type="password"/>
		<map:transform src="Server/Signin/Password.xsl"/>
		<map:serialize type="html5"/>
	</map:match>
	<map:match pattern="Signin.css">
		<map:read mime-type="text/css" src="Server/Signin/Signin.css"/>
	</map:match>
	<map:match pattern="OpenSignin.html" >
		<map:generate type="opensignin"/>
		<map:transform src="Server/Signin/OpenSignin.xsl"/>
		<map:serialize type="html5"/>
	</map:match>
	<map:match pattern="Signout.html" >
		<map:generate type="signout"/>
		<map:transform src="Server/Signin/Signin.xsl"/>
		<map:serialize type="html5"/>
	</map:match>
	<map:match pattern="*.png">
		<map:read mime-type="image/png" src="Server/Signin/{1}.png" />
	</map:match>


	<map:match pattern="Vicar.html" >
		<map:generate type="vicar"/>
		<map:transform src="Server/Core/Vicar.xsl"/>
		<map:serialize type="html5"/>
	</map:match>
	<map:match pattern="Vicar.css">
		<map:read mime-type="text/css" src="Server/Core/Vicar.css"/>
	</map:match>

	<map:match pattern="SchemaList.js" >
		<map:read mime-type="text/javascript" src="Server/Core/SchemaList.js"/>
	</map:match>
<!--
	<map:match pattern="modernizr.js" >
		<map:read mime-type="text/javascript" src="Server/Core/modernizr.js" />
	</map:match>
-->
	<map:redirect-to uri="Error.html"/>
</map:match>

<map:handle-errors>
	<map:select type="exception">
		<map:when test="resourceNotFound">
			<map:read mime-type="text/html" src="Server/Error.html"/>
		</map:when>
		<map:when test="not-found">
			<map:read mime-type="text/html" src="Server/Error.html"/>
		</map:when>
		<map:when test="invalid-session">
			<map:read mime-type="text/html" src="Server/Error.html"/>
		</map:when>
		<map:otherwise>
			<map:read mime-type="text/html" src="Server/Error.html"/>
		</map:otherwise>
	</map:select>
</map:handle-errors>

</map:pipeline>
</map:pipelines>
</map:sitemap>


