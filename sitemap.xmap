<?xml version="1.0" encoding="UTF-8"?>
<map:sitemap xmlns:map="http://apache.org/cocoon/sitemap/1.0">
<map:components>

	<map:generators default="file">
		<map:generator name="opensignin" src="Server.OpenSignin.OpenSignin"/>
		<map:generator name="filemanager" src="Server.Core.FileManager"/>
		<map:generator name="filedownload" src="Server.Core.FileDownload"/>
		<map:generator name="ajax-upload" src="Server.Upload.AjaxServer"/>
		<map:generator name="monitor-server" src="Server.Progress.MonitorServer"/>
	</map:generators>


	<map:transformers default="xslt"/>
	<map:readers default="resource"/>

	<map:serializers default="html">
		<!--script elements need inner text of &#160; to work with serializer - weird -->
		<map:serializer name="html5"
			mime-type="text/html"
			src="org.apache.cocoon.serialization.XMLSerializer">
			<doctype-public></doctype-public>
			<doctype-system>about:legacy-compat</doctype-system>
			<encoding>UTF-8</encoding>
			<indent>yes</indent>
		</map:serializer>
	</map:serializers>

	<map:actions />

	<map:selectors default="browser"/>
	<map:matchers default="wildcard"/>
</map:components>


<map:pipelines>
<map:pipeline>

<!--TOP-->
<map:match pattern="" >
	<!--GO TO INDEX.HTML UNDER APACHE-->
	<map:redirect-to uri="../"/>
</map:match>
<map:match pattern="/" >
	<!--GO TO INDEX.HTML UNDER APACHE-->
	<map:redirect-to uri="../"/>
</map:match>

<!--MODERNIZR-->
<map:match pattern="Modernizr/**">
	<map:match pattern="Modernizr/modernizr.js" >
		<map:read mime-type="text/javascript" src="Server/Modernizr/modernizr.js" />
	</map:match>
</map:match>


<!--OPENSIGNIN-->
<map:match pattern="OpenSignin/**">
	<map:match pattern="OpenSignin/OpenSignin.html" >
		<map:generate type="opensignin"/>
		<map:transform src="Server/OpenSignin/OpenSignin.xsl"/>
		<map:serialize type="html5"/>
	</map:match>
	<map:match pattern="OpenSignin/OpenSignin.xml" >
		<map:generate type="opensignin"/>
		<map:serialize type="xml"/>
	</map:match>
	<map:match pattern="OpenSignin/*.png">
		<map:read mime-type="image/png" src="Server/OpenSignin/{1}.png" />
	</map:match>
	<map:match pattern="OpenSignin/OpenSignin.css">
		<map:read mime-type="text/css" src="Server/OpenSignin/OpenSignin.css"/>
	</map:match>
	<map:redirect-to uri="../Error.html"/>
</map:match>


<!--CORE-->
<map:match pattern="Core/**">

	<!--MAIN FUNCTIONALITY-->
	<map:match pattern="Core/FileManager.html" >
		<map:generate type="filemanager"/>
		<map:transform src="Server/Core/FileManager.xsl"/>
		<map:serialize type="html5"/>
	</map:match>
	<map:match pattern="Core/FileManager.xml" >
		<map:generate type="filemanager"/>
		<map:serialize type="xml"/>
	</map:match>
	<map:match pattern="Core/FileManager.css">
		<map:read mime-type="text/css" src="Server/Core/FileManager.css"/>
	</map:match>

	<map:match pattern="Core/SchemaList.js" >
		<map:read mime-type="text/javascript" src="Server/Core/SchemaList.js"/>
	</map:match>


	<map:redirect-to uri="../Error.html"/>
</map:match>

<!--VALID-->
<map:match pattern="valid/**">
	<map:match pattern="valid/*/*.html" >
		<map:generate type="filedownload"/>
		<map:serialize type="html" />
	</map:match>
</map:match>

<!--DOWNLOAD-->
<map:match pattern="Download/**">
	<map:match pattern="Download/*/*.xml" >
		<map:generate type="filedownload" />
		<map:serialize type="xml" mime-type="application/octet-stream" />
	</map:match>
	<map:match pattern="Download/*/*.zip" >
		<map:generate type="filedownload" />
		<map:serialize type="zip" mime-type="application/octet-stream" />
	</map:match>
	<map:match pattern="Download/*/*.tar" >
		<map:generate type="filedownload" />
		<map:serialize mime-type="application/octet-stream" />
	</map:match>
	<map:match pattern="Download/*/*.tar.gz" >
		<map:generate type="filedownload" />
		<map:serialize mime-type="application/octet-stream" />
	</map:match>
	<map:redirect-to uri="../Error.html"/>
</map:match>

<!--UPLOAD-->
<map:match pattern="Upload/**">

	<!--MULTIPLE FILE UPLOAD VIA AJAX-->
	<map:match pattern="Upload/AjaxUpload.js">
		<map:read mime-type="text/javascript" src="Server/Upload/AjaxUpload.js"/>
	</map:match>
	<map:match pattern="Upload/AjaxUpload.css">
		<map:read mime-type="text/css" src="Server/Upload/AjaxUpload.css"/>
	</map:match>
	<map:match pattern="Upload/AjaxServer.xml" >
		<map:generate type="ajax-upload"/>
		<map:serialize type="xml"/>
	</map:match>

	<map:redirect-to uri="../Error.html"/>
</map:match>

<!--PROG-->
<map:match pattern="Progress/**">
	<map:match pattern="Progress/Monitor.html">
		<map:read mime-type="text/html" src="Server/Progress/Monitor.html"/>
	</map:match>
	<map:match pattern="Progress/MonitorServer.xml" >
		<map:generate type="monitor-server"/>
		<map:serialize type="xml"/>
	</map:match>
	<map:match pattern="Progress/MonitorServer.html" >
		<map:generate type="monitor-server"/>
		<map:transform src="Server/Progress/MonitorServer.xsl"/>
		<map:serialize type="html"/>
	</map:match>
	<map:match pattern="Progress/Monitor.js" >
		<map:read mime-type="text/javascript" src="Server/Progress/Monitor.js"/>
	</map:match>
	<map:match pattern="Progress/Monitor.css">
		<map:read mime-type="text/css" src="Server/Progress/Monitor.css"/>
	</map:match>

	<map:redirect-to uri="../Error.html"/>
</map:match>

<map:match pattern="Error.html">
	<map:match pattern="Error.html">
		<map:read mime-type="text/html" src="Server/Error.html"/>
	</map:match>
</map:match>

<map:handle-errors>
	<map:select type="exception">
		<map:when test="resourceNotFound">
			<map:read mime-type="text/html" src="Server/Error.html"/>
		</map:when>
		<map:when test="not-found">
			<map:read mime-type="text/html" src="Server/Error.html"/>
		</map:when>
		<map:when test="invalid-session">
			<map:read mime-type="text/html" src="Server/Error.html"/>
		</map:when>
		<map:otherwise>
			<map:read mime-type="text/html" src="Server/Error.html"/>
		</map:otherwise>
	</map:select>
</map:handle-errors>

</map:pipeline>
</map:pipelines>
</map:sitemap>


